
public class number {
	public static int check(int n)
	{
		int res=0;
		
		while(n>0)
		{
			int rem=n%10;
			res= res+rem*rem;
			n= n/10;
		}
		return res;
		
	}
	public static int check2(int n)
	{
		if(n<9)
		{
			return n;
		}
		while(check(n)>9)
		{
			check(n);
		}
		return n;
	}
public static void main(String[] args) {
	int n= 19;
	System.out.println(check(n));
	
}
}
