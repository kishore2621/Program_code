package org.jsp.QueuimplLikedlist;

class Node
{
	Object data;
	Node next;
	
	
	public Node(Object data)
	{
		this.data=data;
	}
	
}

public class QueuimplLikedlist 
{
	Node head;
	Node tail;
	
	@Override
	public String toString()
	{if(head==null)
	{
		return "[]";
	}
	String res="[";
	
	Node temp=head;
	
	while(temp!=null)
	{
		res=res+temp.data;
//		System.out.print(temp.data+" ");
	}
		
	res=res+"]";
		
		return res;
		
	}
	
	
	
	public void add(Object data)
	{
		Node newNode=new Node (data);
		
		if(head==null)
		{
			head=newNode;
			tail =newNode;
			return;
		}
		tail.next=newNode;
		tail=newNode;
	}
	
//	public void display()
//	{
//		Node temp=head;
//		
//		while(temp!=null)
//		{
//			System.out.print(temp.data+" ");
//			temp=temp.next;
//		}
//		System.out.println();
//	}
	
	public Object poll()
	{
		if(head==null)
		{
			System.out.println("empty list");
			return -1;
		}
		Node temp=head;
		Object data=temp.data;
		head=head.next;
		temp=null;
		return data;
	}
	
	public boolean isEmpty()
	{
		return head==null;
	}
	
	public Object peak()
	{
		return head.data;
	}
	
	public int size ()
	{
		Node temp=head;
		int count =0;
		while (temp!=null)
		{
			count++;
			temp=temp.next;
		}
		return count;
	}
}
