package org.jsp.QueuimplLikedlist;

class Node
{
	int data;
	Node next;
	
	
	public Node(int data)
	{
		this.data=data;
	}
	
}

public class QueuimplLikedlist 
{
	Node head;
	Node tail;
	
	
	public void add(int data)
	{
		Node newNode=new Node (data);
		
		if(head==null)
		{
			head=newNode;
			tail =newNode;
			return;
		}
		tail.next=newNode;
		tail=newNode;
	}
	
	

}
